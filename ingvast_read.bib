@Article{Kuo:2001:SMB,
  author =  {Arthur D. Kuo},
  title = {A simple model of bipedal walking predicts the preffered speed --
	   step length relationship},
  journal = j-asme-biomech-eng,
  year =  {2001},
  volume =  {123},
  pages = {264--269},
  abstract =  {We used a simple model of passive dynamic walking, with the
      addition of active powering on level ground, to study the preferred
      relationship between speed and step lengh in humans.  We tested several
      hypothetical metabolic costs, with one component proportional to the
      mechanical work associated with pushing off with the stance leg at
      toe-off, and ontother component associated with several possible costs of
      forcing oscillations of the swing leg.  For this second component, a cost
      based on the amount of force needed to oscilate the leg divided by the
      time duration of that force predicts the preferred speed-step length
      relationship much better than other costs, such as the amount of
      mechanical work done in swinging the leg.  The cost of force/time models
      the need to recruit fast muscle fibres for large forces at short
      durations.  The actual mechanical work performed by muscles on the swing
      leg appears to be of relatively less importance, although it appears to be
      minimized by the use of short bursts of muscle activity in near-isometric
      conditioins.  The combined minimization of toe-off mechanical work and
      force divided by time predicts the preferred speed-step length
      relationship.},
}

